{% extends 'layout.html.twig' %}

{% block content %}
      <div class="starter-template">
        <h1>Dead translations</h1>

        <table class="table table-bordered table-hover table-condensed">
            <thead>
                <tr>
                    <th>Keys</th>
                </tr>
            </thead>
            <tbody>
                {% for key in deads %}
                <tr>
                    <td class="text-left">{{ key }}</td>
                </tr>
                {% endfor %}
            </tbody>
        </table>
      </div>
{% endblock %}